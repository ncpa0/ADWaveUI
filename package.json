{
  "name": "adwaveui",
  "version": "0.0.4",
  "main": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/esm/index.mjs",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/components/input/input": {
      "default": "./dist/esm/components/input/input.mjs",
      "types": "./dist/types/components/input/input.d.ts"
    },
    "./dist/components/selector/selector": {
      "default": "./dist/esm/components/selector/selector.mjs",
      "types": "./dist/types/components/selector/selector.d.ts"
    },
    "./dist/components/slider/slider": {
      "default": "./dist/esm/components/slider/slider.mjs",
      "types": "./dist/types/components/slider/slider.d.ts"
    },
    "./dist/components/switch/switch": {
      "default": "./dist/esm/components/switch/switch.mjs",
      "types": "./dist/types/components/switch/switch.d.ts"
    }
  },
  "keywords": [
    "adwaita",
    "libadwaita",
    "gnome",
    "gtk",
    "ui",
    "css",
    "web components",
    "input",
    "selector",
    "slider",
    "switch"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ncpa0/ADWaveUI"
  },
  "description": "Interactive Web Components inspired by the Gtk Adwaita theme.",
  "license": "MIT",
  "author": {
    "name": "Szymon Bretner",
    "email": "szymonb21@gmail.com"
  },
  "scripts": {
    "fix:format": "prettier -w ./src/**/*.ts",
    "test:format": "prettier -c ./src/**/*.ts",
    "test:tsc": "tsc --noEmit",
    "build": "node ./scripts/build.mjs",
    "prepare": "husky install"
  },
  "packageManager": "yarn@1.22.19",
  "devDependencies": {
    "@ncpa0cpl/nodepack": "^2.3.3",
    "csso": "^5.0.5",
    "esbuild": "^0.20.2",
    "git-hook-tasks": "git+https://github.com/ncpa0cpl/git-hook-tasks#0.0.1",
    "husky": "^8.0.3",
    "node-os-walk": "^1.0.2",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "adwavecss": ">=0.0.13",
    "jsxte": "^3.3.1",
    "jsxte-wc": "^1.1.5"
  }
}
